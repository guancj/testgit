<?xml version="1.0" encoding="utf-8" ?>
<Function id="IDT_ALS_HLA_StartTest_Service_1" description="发送开始检测指令，service 1.">
<Inputs>
<Variable id="pHndl" type="3" data="0" description="通讯句柄"/>
</Inputs>
<Outputs>
<Variable id="ret" type="3" data="0" description="返回值,0:没有正在被执行的service;1:执行成功;2:service正在执行中;-1:执行失败"/>
</Outputs>
<Script>ret = LibIDT.idtSendDatagramALS("\x01",LibIDT.idtGetTimeout(),pHndl);</Script></Function>

