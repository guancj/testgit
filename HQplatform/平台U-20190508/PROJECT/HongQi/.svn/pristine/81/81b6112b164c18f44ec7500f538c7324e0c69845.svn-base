<?xml version="1.0" encoding="utf-8" ?>
<Function id="VW2000_Communication_Connect">
<Outputs>
<Variable id="vciExData" type="8" data="" description="vw2000 protocal select Data"/>
<Variable id="ECU_Handle" type="8" data="" description="ECU Handle"/>
</Outputs>
<Script>--[[*****************************************************************************
this function is used to   connect ECU of vw2000  
output parameter:vciExData;ECU_Handle

01/21/2016     created by viforce
11/03/2016     changed by viforce
******************************************************************************--]]
vciExData = "";

ECU_Handle = engine.EcuOpen();

local count = 3;
while(0 ~= engine.LastError ) do
engine.Delay(1000);
ECU_Handle = engine.EcuOpen();
count = count - 1;
if(count &lt;= 0) then
break;
end;
end;

if (ECU_Handle  == nil)  then 
	ECU_Handle = -1;
	engine.LastError = 1;
	engine.StatValue = "ECU Handle nil ";
	return vciExData,ECU_Handle
end





#COMMEX ECU_Handle -1 0x31 0xb8 0x00 0x00

if(0 ~= engine.LastError) then
engine.Delay(2000);
#COMMEX ECU_Handle -1 0x31 0xb8 0x00 0x00

end;


if(0 == engine.LastError ) then

vciExData = @0-;

else

local recel = @0-;
engine.StatValue = "ERR :"..tostring(recel);

end;




</Script></Function>

