<Activity id="34" ActivityType="3" name="高度值运算">
<Rect left="114" top="153" right="214" bottom="185"/>
<Script>engine.println(" &lt;FAS_61_LDW_HeightPanel&gt; : IDT_YPosRadhausVL --&gt;"..tostring(IDT_YPosRadhausVL))
engine.println(" &lt;FAS_61_LDW_HeightPanel&gt; : IDT_YPosRadhausVR --&gt;"..tostring(IDT_YPosRadhausVR))
engine.println(" &lt;FAS_61_LDW_HeightPanel&gt; : IDT_YPosRadhausHL --&gt;"..tostring(IDT_YPosRadhausHL))
engine.println(" &lt;FAS_61_LDW_HeightPanel&gt; : IDT_YPosRadhausHR --&gt;"..tostring(IDT_YPosRadhausHR))
engine.println(" &lt;FAS_61_LDW_HeightPanel&gt; : X-Koordinate LDW-Tafel unten links --&gt;"..tostring(fFAS_LDW_UL_X))
-- [AU571CN]
SocketX=-851.1
XRadstand=3012.0
Z_VA_nom=699.0
Z_HA_nom=702.0
Z_Tafel_nom=1200.0
engine.println(" &lt;FAS_61_LDW_HeightPanel&gt; : XRadstand --&gt;"..tostring(XRadstand))
engine.println(" &lt;FAS_61_LDW_HeightPanel&gt; : Z_VA_nom --&gt;"..tostring(Z_VA_nom))
engine.println(" &lt;FAS_61_LDW_HeightPanel&gt; : Z_HA_nom --&gt;"..tostring(Z_HA_nom))
engine.println(" &lt;FAS_61_LDW_HeightPanel&gt; : Z_Tafel_nom --&gt;"..tostring(Z_Tafel_nom))
-- process 
if(SocketX &lt; 0.0) then
SocketX = SocketX * (-1.0)
end

local deltaZ_VA = ((IDT_YPosRadhausVR+IDT_YPosRadhausVL)/2.0) - Z_VA_nom
local deltaZ_HA = ((IDT_YPosRadhausHR+IDT_YPosRadhausHL)/2.0) - Z_HA_nom

local deltaZ1 = deltaZ_VA + (deltaZ_HA-deltaZ_VA)*(SocketX/XRadstand)
local deltaZ2 = (deltaZ_VA-deltaZ_HA) * (fFAS_LDW_UL_X+SocketX) / XRadstand

HoeheTafel = Z_Tafel_nom + deltaZ1 + deltaZ2

engine.println(" &lt;FAS_61_LDW_HeightPanel&gt; : HoeheTafel --&gt;"..tostring(HoeheTafel))</Script></Activity>
