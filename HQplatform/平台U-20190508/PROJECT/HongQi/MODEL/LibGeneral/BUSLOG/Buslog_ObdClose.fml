<?xml version="1.0" encoding="utf-8" ?>
<Function>
<Inputs>
<Variable id="iObdHandle" type="3" data="0" description="通讯句柄"/>
</Inputs>
<Script>if(iObdHandle == nil or iObdHandle &lt;0) then
	engine.LastError = 1;
