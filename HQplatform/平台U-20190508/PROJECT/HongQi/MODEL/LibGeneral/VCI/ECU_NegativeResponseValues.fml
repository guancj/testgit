elseif(NegativeResonseValue == 0x83) then
	tmpStatValue = "engineIsRunning";
elseif(NegativeResonseValue == 0x84) then
	tmpStatValue = "engineIsNotRunning";
elseif(NegativeResonseValue == 0x85) then
	tmpStatValue = "engineRunTimeTooLow";
elseif(NegativeResonseValue == 0x86) then
