elseif(NegativeResonseValue == 0x8B) then
	tmpStatValue = "throttle/PedalTooLow";
elseif(NegativeResonseValue == 0x8C) then
	tmpStatValue = "transmissionRangeNotInNeutral";
elseif(NegativeResonseValue == 0x8D) then
	tmpStatValue = "transmissionRangeNotInGear";
elseif(NegativeResonseValue == 0x8F) then
	tmpStatValue = "brakeSwitch(es)NotClosed (Brake Pedal not pressed or not applied)";
elseif(NegativeResonseValue == 0x90) then
	tmpStatValue = "shifterLeverNotInPark";
elseif(NegativeResonseValue == 0x91) then
	tmpStatValue = "torqueConverterClutchLocked";
elseif(NegativeResonseValue == 0x92) then
