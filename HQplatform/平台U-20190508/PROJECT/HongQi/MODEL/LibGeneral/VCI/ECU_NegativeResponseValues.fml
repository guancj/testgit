<?xml version="1.0" encoding="utf-8" ?>
<Function>
<Inputs>
<Variable id="NegativeResonseValue" type="3" data="00" description="如0x11"/>
</Inputs>
<Outputs>
<Variable id="responseCode" type="8" data="" description="如 &apos;serviceNotSupported&apos;"/>
</Outputs>
<Script>--[[*****************************************************************************
this function is used to  Translate Negative Code 
input parameter:NegativeResonseValue
output parameter:responseCode

11/08/2016     created by viforce
******************************************************************************--]]

local tmpStatValue = "ISOSAEReserved "
if(NegativeResonseValue == 0x11) then
	tmpStatValue = "serviceNotSupported";
elseif(NegativeResonseValue == 0x12) then
	tmpStatValue = "subFunctionNotSupported";
elseif(NegativeResonseValue == 0x13) then
	tmpStatValue = "incorrectMessageLengthOrInvalidFormat";
elseif(NegativeResonseValue == 0x14) then
	tmpStatValue = "responseTooLong";
elseif(NegativeResonseValue == 0x21) then
	tmpStatValue = "busyRepeatRequest";
elseif(NegativeResonseValue == 0x22) then
	tmpStatValue = "conditionsNotCorrect";
elseif(NegativeResonseValue == 0x24) then
	tmpStatValue = "requestSequenceError";
elseif(NegativeResonseValue == 0x31) then
	tmpStatValue = "requestOutOfRange";
elseif(NegativeResonseValue == 0x33) then
	tmpStatValue = "securityAccessDenied";
elseif(NegativeResonseValue == 0x35) then
	tmpStatValue = "invalidKey";
elseif(NegativeResonseValue == 0x36) then
