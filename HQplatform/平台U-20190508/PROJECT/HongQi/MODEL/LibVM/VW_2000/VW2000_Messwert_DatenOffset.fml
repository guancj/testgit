<?xml version="1.0" encoding="utf-8" ?>
<Function>
<Inputs>
<Variable id="LocalIdent" type="3" data="0" description="group ID"/>
<Variable id="ECU_Handle" type="8" data="vciHandle" description="ECU Handle"/>
<Variable id="DataOffset" type="3" data="1" description="DataOffset"/>
</Inputs>
<Outputs>
<Variable id="RecvTel" type="8" data="" description="group data"/>
</Outputs>
<Script>--[[*****************************************************************************
this is used to read by LocalIdent
input parameter:LocalIdent;ECU_Handle;DataOffset;
output parameter:RecvTel;

01/14/2016     created by viforce
11/03/2016     changed by viforce
******************************************************************************--]]

RecvTel = "";

if (ECU_Handle  == nil)  then 
	engine.LastError = 1;
	engine.StatValue = "ECU Handle nil ";
	return RecvTel
end

#COMMEX ECU_Handle -1 0x21 LocalIdent


if (0 == engine.LastError) then

RecvTel = @1-;
RecvTel = string.sub(RecvTel,DataOffset,-1);
end

engine.println("Receive :"..LibGeneral.hex2str(RecvTel));</Script></Function>

